//CPF MASK
SET mascaraCPF = IF(NOT ISNULL($1), LEFT(NUM($1, '00000000000'), 3) & '.' & MID(NUM($1, '00000000000'), 4, 3)  & '.' &  MID(NUM($1, '00000000000'), 8, 3) & '-' & RIGHT(NUM($1, '00000000000'), 2));

//CNPJ MASK
SET mascaraCNPJ = IF(NOT ISNULL($1), LEFT(NUM([CNPJ], '00000000000000'), 2) & '.' & MID(NUM([CNPJ], '00000000000000'), 3, 3) & '.' & MID(NUM([CNPJ], '00000000000000'), 6, 3) & '/' & MID(NUM([CNPJ], '00000000000000'), 9, 4) & '-' & RIGHT([CNPJ], 2));

//CEP MASK
SET mascaraCEP = IF(NOT ISNULL($1), LEFT(NUM( $1, '00000000'), 2) & '.' & MID(NUM( $1, '00000000'), 3, 3) & '-' & RIGHT(NUM( $1, '00000000'), 3));